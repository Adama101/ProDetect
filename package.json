{
  "name": "prodetect",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 9002",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "db:setup": "psql -h $DB_HOST -U $DB_USER -d $DB_NAME -f src/lib/database/schema.sql",
    "db:seed": "psql -h $DB_HOST -U $DB_USER -d $DB_NAME -f src/lib/database/seed.sql",
    "db:migrate": "npm run db:setup && npm run db:seed"
